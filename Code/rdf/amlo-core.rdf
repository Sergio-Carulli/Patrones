        <rdfs:comment xml:lang="en">the weight or significance assigned to the reputational risk dimension in a risk assessment or rating process.</rdfs:comment>
        <rdfs:label xml:lang="en">reputational risk weight</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#riskDimensionEstimator -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#riskDimensionEstimator">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/amlo/core#hasRiskEstimator"/>
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#RiskProfile"/>
        <rdfs:comment xml:lang="en">a generic property used to define common characteristics for profile dimension estimators</rdfs:comment>
        <rdfs:label xml:lang="en">risk dimension estimator</rdfs:label>
