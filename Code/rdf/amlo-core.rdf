    <!-- http://w3id.org/amlo/core#operatesInCountry -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#operatesInCountry">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/Person"/>
