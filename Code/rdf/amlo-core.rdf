        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#InconsistentCDD"/>
        <rdfs:comment xml:lang="en">relates an InconsistenceCDD annotation with the data provided by customer due diligence process</rdfs:comment>
        <rdfs:label xml:lang="en">customer due diligence data</rdfs:label>
    </owl:ObjectProperty>
    


