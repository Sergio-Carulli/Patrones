        <rdfs:label xml:lang="en">criminal risk</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#criminalRiskWeight -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#criminalRiskWeight">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/amlo/core#hasRiskDimensionWeight"/>
