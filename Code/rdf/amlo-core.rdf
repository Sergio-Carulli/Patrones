        <rdfs:subPropertyOf rdf:resource="http://w3id.org/amlo/core#hasNominalHeat"/>
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#RiskProfileImpact"/>
        <rdfs:comment xml:lang="en">the nominal heat value expressed in a unit of measurement for a specific system or component</rdfs:comment>
        <rdfs:label xml:lang="en">has nominal heat</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#hasReferenceFamilyName -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#hasReferenceFamilyName">
