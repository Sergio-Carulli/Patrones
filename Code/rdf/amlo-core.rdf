        <rdfs:label xml:lang="en">criminal risk weight</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#fiscalRisk -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#fiscalRisk">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/amlo/core#riskDimensionEstimator"/>
