        <rdfs:range rdf:resource="http://w3id.org/amlo/core#RiskDistribution"/>
        <rdfs:comment xml:lang="en">The weight assigned to a specific risk dimension within a risk profile or assessment</rdfs:comment>
        <rdfs:label xml:lang="en">has risk dimension weight</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#hasRiskEstimator -->

