  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(255, 255, 255, 0.2);
}
.ensModal a:hover:not(:focus) {
  background-color: rgba(194, 202, 207, 0.2);
}
.ensModal hr {
  border: 0;
  height: 2px;
  background: #e0e4e7;
  margin-top: 16px;
}
.ensModal .ensTitle {
  position: sticky;
  z-index: 1;
  top: 0;
  font-size: 20px;
  line-height: 24px;
  font-weight: bold;
  letter-spacing: -0.015em;
  text-align: center;
  background: #ffffff;
  border-bottom: solid 1px #dfe4e8;
