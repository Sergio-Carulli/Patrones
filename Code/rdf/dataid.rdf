    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">identifier scheme</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/spar/datacite/IdentifierScheme"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#inCatalog">
    <rdfs:comment xml:lang="en">The inverse property of dcat:record, pointing back to the dcat:Catalog in which this DataId is recorded.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#DataId"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">in catalog</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#Catalog"/>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/dcat#record"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#innerMediaType">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Points out the MediaType of the (compressed) file inside another file (relevant for archive files).</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">inner media type</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/dcat#mediaType"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#isDistributionOf">
    <rdfs:comment xml:lang="en">Inverse property of dcat:distribution, linking a Distribution to a Dataset.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Distribution"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">is distribution of</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#isInheritable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Indicates whether this Authorization will be valid after an update to the DataID or a scope element.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Authorization"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">is inheritable</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#latestVersion">
    <rdfs:comment xml:lang="en">Latest version of a DataId/Dataset/Distribution.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">latest version</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Entity"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#literal">
    <rdfs:comment xml:lang="en">An actual textual statement or literal (string).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">statement</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.essepuntato.it/2010/06/literalreification/hasLiteralValue"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#needsSpecialAuthorization">
    <rdfs:comment xml:lang="en">Points out an Authorization which grants some degree of authority over this resource to the exclusion of other Authorizations which are not referred with this property.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">needs special authorization</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Authorization"/>
    <rdfs:subPropertyOf rdf:resource="http://dataid.dbpedia.org/ns/core#underAuthorization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#nextVersion">
    <rdfs:comment xml:lang="en">Next version of a DataId/Dataset/Distribution.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">next version</rdfs:label>
    <owl:inverseOf rdf:resource="http://dataid.dbpedia.org/ns/core#previousVersion"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#openness">
    <rdfs:comment xml:lang="en">General description of how data will be shared. For example embargo periods (if any), outlines of technical mechanisms for dissemination or a definition of whether access will be widely open or restricted to specific groups. In case the Dataset cannot be shared, the reasons for this should be mentioned (e.g. ethical, rules of personal data, intellectual property, commercial, privacy-related, security-related).</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/dmp#"/>
    <rdfs:label xml:lang="en">openness</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#SimpleStatement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#preview">
    <rdfs:comment xml:lang="en">Provides the URL of a short preview of the data provided by a Distribution, helpful when conveying type and format of the data provided as an example.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Distribution"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">preview</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#previousVersion">
    <rdfs:comment xml:lang="en">Previous version of a DataId/Dataset/Distribution.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">previous version</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#qualifiedDatasetRelation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:comment xml:lang="en">Qualifies the dataid:relatedDatset property with a DatasetRelationship.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">relation of</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#DatasetRelationship"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#qualifiedInfluence"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#qualifiedRelationOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Inverse property of dataid:qualifiedDatasetRelation, pointing out a dataset which has related datasets referred to via dataid:dataid:qualifiedRelationTo.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">qualified relation of</rdfs:label>
    <owl:inverseOf rdf:resource="http://dataid.dbpedia.org/ns/core#qualifiedDatasetRelation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#qualifiedRelationTo">
    <rdfs:comment xml:lang="en">Pointing out datasets which are somehow related to the dataset referred to via dataid:dataid:qualifiedRelationOf.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#DatasetRelationship"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">qualified relation to</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#relatedDataset">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:comment xml:lang="en">Points to other Datasets containing related data. Can be qualified with dataid:DatasetRelationship. (note: while this property is symmetric, its qualification is not)</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">related dataset</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
    <owl:propertyChainAxiom>
      <rdf:Description>
        <rdf:first rdf:resource="http://dataid.dbpedia.org/ns/core#qualifiedDatasetRelation"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first rdf:resource="http://dataid.dbpedia.org/ns/core#qualifiedRelationTo"/>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:Description>
        </rdf:rest>
      </rdf:Description>
    </owl:propertyChainAxiom>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#reuseAndIntegration">
    <rdfs:comment xml:lang="en">Information on the possibilities for integration and reuse of the Dataset.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/dmp#"/>
    <rdfs:label xml:lang="en">reuse and integration</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#SimpleStatement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#similarData">
    <rdfs:comment xml:lang="en">Information on the existence (or absence) of similar data (see also dataid:similarDataset).</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/dmp#"/>
    <rdfs:label xml:lang="en">similar data</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#SimpleStatement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#softwareRequirement">
    <rdfs:comment xml:lang="en">Software needed to access the data provided via this Distribution or otherwise relevant for consuming the data.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Distribution"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">software requirement</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#SimpleStatement"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#typeExtension">
    <rdfs:comment xml:lang="en">Lists file extensions commonly used with this MediaType.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">media type file extension</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#typeName">
    <rdfs:comment xml:lang="en">Names the MediaType described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">media type name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#typeReference">
    <rdfs:comment xml:lang="en">Refers to a standard document of the MediaType described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">media type reference</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/page"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#typeTemplate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">The template (or mime type string) of a MediaType.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#MediaType"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">media type template</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#uncompressedByteSize">
    <rdfs:comment xml:lang="en">Records the byte size of the uncompressed content of an archive file.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Distribution"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">uncompressed byte size</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/dcat#byteSize"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#underAuthorization">
    <rdfs:comment xml:lang="en">Points out an Authorization which grants some degree of authority over this resource (inverse property of dataid:authorizationScope).</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">under authorization</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Authorization"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#qualifiedAttribution"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#usefulness">
    <rdfs:comment xml:lang="en">Description of to whom the Dataset could be useful, and whether it underpins a scientific publication.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#Dataset"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/dmp#"/>
    <rdfs:label xml:lang="en">usefulness</rdfs:label>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#SimpleStatement"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#validFrom">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">The influence an Agent has over an Entity is valid from (inclusive) a certain point in time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#AgentInfluence"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">valid from</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://dataid.dbpedia.org/ns/core#validUntil">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">The influence an Agent has over an Entity is valid until (exclusive) a certain point in time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#AgentInfluence"/>
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">valid until</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
