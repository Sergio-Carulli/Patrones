        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>Position of an instant, expressed using xsd:date</rdfs:comment>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time</rdfs:isDefinedBy>
        <rdfs:label>in XSD date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/molecularFormula -->

    <owl:DatatypeProperty rdf:about="https://schema.org/molecularFormula">
        <rdfs:comment xml:lang="en">The empirical formula is the simplest whole number ratio of all the atoms in a molecule.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/"/>
        <rdfs:label xml:lang="en">molecular formula</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ecfo#hasAdditionalContext -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/ecfo#hasAdditionalContext">
        <rdfs:domain rdf:resource="https://w3id.org/ecfo#EmissionConversionFactor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Conversion factors often have subtelties that contextualize the emission source. For example the emission source may be a transportation method (car), but additional context may indicate that the CF is for &quot;large cars&quot;, or lars with more than 120 horse power.
These subteltlies are captured in the &quot;hasAdditionalContext&quot; property</rdfs:comment>
        <rdfs:label xml:lang="en">has additional context</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ecfo#hasUsageNotes -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/ecfo#hasUsageNotes">
        <rdfs:domain rdf:resource="https://w3id.org/ecfo#EmissionConversionFactor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property used to describe any limitations, disclaimers or usage notes associated with a CF. For example &quot;This CF is an estimation based on local data and should be superseeded by governmental sources&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">has usage notes</rdfs:label>
    </owl:DatatypeProperty>
    


