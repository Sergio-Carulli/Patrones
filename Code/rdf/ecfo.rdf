    <!-- http://qudt.org/schema/qudt/abbreviation -->

    <owl:DatatypeProperty rdf:about="http://qudt.org/schema/qudt/abbreviation">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">An abbreviation for a unit is a short ASCII string that is used in place of the full name for the unit in contexts where non-ASCII characters would be problematic, or where using the abbreviation will enhance readability. When a power of abase unit needs to be expressed, such as squares this can be done using abbreviations rather than symbols. For example, sq ft means square foot, and cu ft means cubic foot.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://qudt.org/schema/qudt/"/>
        <rdfs:label xml:lang="en">abbreviation</rdfs:label>
    </owl:DatatypeProperty>
    


