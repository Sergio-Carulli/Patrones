    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2002/07/owl#qualifiedCardinality -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#qualifiedCardinality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.gleif.org/ontology/L2/hasAccountingPeriod -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasAccountingPeriod">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/LegalEntityRelationshipRecord"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Period"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has accounting period</rdfs:label>
        <skos:definition>The dates in this Period indicate the accounting period covered by the most recent validation documents for this relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasAccountingStandard -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasAccountingStandard">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/isQualifiedBy"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/AccountingConsolidation"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/L2/AccountingStandard"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has accounting standard</rdfs:label>
        <skos:definition>The accounting standard applied to determine the definition of e.g. ultimate or direct accounting consolidating parent for the relationship. The relevant accounting standard is that applicable to the &apos;parent&apos; entity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasChild -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasChild">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasSource"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has child</rdfs:label>
        <skos:definition>The entity that plays the child role.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasDocumentFilingPeriod -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasDocumentFilingPeriod">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/LegalEntityRelationshipRecord"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Period"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has document filing period</rdfs:label>
        <skos:definition>The dates in this instance of Period indicate the validity period of a regulatory filing, accounting document, or other document demonstrating the relationship&apos;s validity</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasParent -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasParent">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasTarget"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has parent</rdfs:label>
        <skos:definition>The entity that plays the parent role.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasRelationshipPeriod -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasRelationshipPeriod">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Period"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has relationship period</rdfs:label>
        <skos:definition>The dates in this instance of Period indicate the duration of validity of the relationship itself, as distinct from any administrative or reporting aspects.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasRelationshipStatus -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasRelationshipStatus">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/L2/RelationshipStatus"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has relationship status</rdfs:label>
        <skos:definition>Indicates the status of the relationship (i.e., active, inactive).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasValidationDocuments -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasValidationDocuments">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/LegalEntityRelationshipRecord"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/L2/RelationshipValidationDocumentsKind"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has validation documents</rdfs:label>
        <skos:definition>Type of source document(s) used for validating the relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/L2/hasValidationSources -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/L2/hasValidationSources">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/LegalEntityRelationshipRecord"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/L1/ValidationSourceKind"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has validation sources</rdfs:label>
        <skos:definition>Level of relationship validation.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
