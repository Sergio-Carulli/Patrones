    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.gleif.org/ontology/L2/hasValidationReference -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/L2/hasValidationReference">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/L2/LegalEntityRelationshipRecord"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/L2/"/>
        <rdfs:label>has validation reference</rdfs:label>
        <skos:definition>A reference to a specific document or other source used as the basis of relationship validation for this relationship record.</skos:definition>
    </owl:DatatypeProperty>
    


