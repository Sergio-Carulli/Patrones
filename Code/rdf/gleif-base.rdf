        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.gleif.org/ontology/Base/EntityStatusActive"/>
                    <rdf:Description rdf:about="https://www.gleif.org/ontology/Base/EntityStatusInactive"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
