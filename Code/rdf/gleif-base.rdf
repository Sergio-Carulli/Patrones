<?xml version="1.0"?>
<rdf:RDF xmlns="https://www.gleif.org/ontology/Base/"
     xml:base="https://www.gleif.org/ontology/Base/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:Base="https://www.gleif.org/ontology/Base/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:SpecificationMetadata="http://www.omg.org/techprocess/ab/SpecificationMetadata/">
    <owl:Ontology rdf:about="https://www.gleif.org/ontology/Base/">
        <owl:versionIRI rdf:resource="https://www.gleif.org/ontology/Base-v1.0/Base/"/>
        <owl:imports rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <terms:abstract xml:lang="en">Ontology defining generic concepts for reuse by other Global Legal Entity Identifier Foundation (GLEIF) ontologies. It defines generic classes for (legal) Entities and their relationships and statuses; and generic properties for different types of name and address. It makes use of the OMG Languages Countries and Codes (LCC) ontology (based on the ISO 3166 standard) for country and region information.</terms:abstract>
        <terms:contributor rdf:resource="http://www.workingontologist.com"/>
        <terms:contributor rdf:resource="http://purl.org/bryon"/>
        <terms:contributor rdf:resource="https://www.linkedin.com/in/peterivett/"/>
        <terms:creator rdf:resource="https://www.gleif.org/ontology/Base/GLEIF"/>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-02-01T00:00:00+00:00</terms:issued>
        <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gleif.org/en/meta/lei-data-terms-of-use/</terms:license>
        <terms:rights>Copyright (c) 2019 Global Legal Entity Identifier Foundation (GLEIF)</terms:rights>
        <terms:rightsHolder rdf:resource="https://www.gleif.org/ontology/Base/GLEIF"/>
        <terms:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</terms:source>
        <terms:title xml:lang="en">Global Legal Entity Identifier Foundation Base Ontology</terms:title>
        <vann:preferredNamespacePrefix>gleif-base</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://www.gleif.org/ontology/Base/</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">GLEIF Base Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/rightsHolder -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/rightsHolder"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- https://www.gleif.org/ontology/Base/hasAbbreviation -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasAbbreviation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
    </owl:AnnotationProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasLegalName -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasLegalName"/>
    


    <!-- https://www.gleif.org/ontology/Base/hasName -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
    </owl:AnnotationProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasNameAdditional -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasNameAdditional">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
    </owl:AnnotationProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasNameLegalLocal -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasNameLegalLocal">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    </owl:AnnotationProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasNameTransliterated -->


    <owl:AnnotationProperty rdf:about="https://www.gleif.org/ontology/Base/hasNameTransliterated">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.gleif.org/ontology/Base/hasAddress -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddress">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address</rdfs:label>
        <skos:definition>Has a means by which the entity may be located or contacted or may receive correspondence.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressHeadquarters -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressHeadquarters">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address headquarters</rdfs:label>
        <skos:definition>An address of the headquarters of the Entity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressLegal -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressLegal">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address legal</rdfs:label>
        <skos:definition>An address of the entity as recorded in the registration of the entity in its legal jurisdiction.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressTransliterated -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressTransliterated">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAddress"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddressASCII"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address transliterated</rdfs:label>
        <skos:definition>An address which has been converted from original to western text.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasCountry -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasCountry">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasGeographicRegion"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/Country"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has country</rdfs:label>
        <skos:definition>The country, based on the 2-character ISO 3166-1 country code.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasCoverageArea -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasCoverageArea">
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has coverage area</rdfs:label>
        <skos:definition>Indicates a geographic region in which some service is provided, or to which some policy applies, or in which something is available.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasEntityExpirationReason -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasEntityExpirationReason">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/EntityExpirationReason"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has entity expiration reason</rdfs:label>
        <skos:definition>The reason that an entity ceased to exist and/or operate.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasEntityStatus -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasEntityStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/EntityStatus"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has entity status</rdfs:label>
        <skos:definition>Indicates the status of the entity (i.e., active, inactive).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasGeographicRegion -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasGeographicRegion">
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has geographic region</rdfs:label>
        <skos:definition>The country or region where something is located.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasLegalJurisdiction -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasLegalJurisdiction">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has legal jurisdiction</rdfs:label>
        <skos:definition>The jurisdiction of legal formation and registration of the entity (and upon which the LegalForm data element is also dependent). </skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasRegion -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasRegion">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasGeographicRegion"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/CountrySubdivision"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has region</rdfs:label>
        <skos:altLabel>has state</skos:altLabel>
        <skos:altLabel>has subdivision</skos:altLabel>
        <skos:definition>The (optional) region or subdivision (state, province, region, etc.) based on the 4- to 6-character ISO 3166-2 region code</skos:definition>
        <skos:prefLabel>has region</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasRegistrationStatus -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasRegistrationStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/RegistrationStatus"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has registration status</rdfs:label>
        <skos:definition>indicates the status of a specific registration, such as for an identifier or license</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasSource -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasSource">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has source</rdfs:label>
        <skos:definition>The entity that is the source of the directed relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasSuccessor -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasSuccessor">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has successor</rdfs:label>
        <skos:definition>The surviving/new entity which continues/replaces this registration.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasTarget -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/hasTarget">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Entity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has target</rdfs:label>
        <skos:definition>The entity that plays the target of the directed relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/identifies -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/identifies">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>identifies</rdfs:label>
        <skos:definition>The relationship between an identifier and the thing it uniquely identifies.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/isManagedBy -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/isManagedBy">
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>is managed by</rdfs:label>
        <skos:definition>Relates something to another thing that has some role in directing its affairs.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/isQualifiedBy -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/isQualifiedBy">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/LegalEntityRelationship"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>is qualified by</rdfs:label>
        <skos:definition>Any additional qualitative properties that help to categorize the relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/isRegisteredIn -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/isRegisteredIn">
        <rdfs:range rdf:resource="https://www.gleif.org/ontology/Base/Registry"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>is registered in</rdfs:label>
        <skos:definition>indicates the registry that something is registered in</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.gleif.org/ontology/Base/records -->


    <owl:ObjectProperty rdf:about="https://www.gleif.org/ontology/Base/records">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/RegistryEntry"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>records</rdfs:label>
        <skos:definition>The thing which the registry entry is a record of.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/created -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/modified -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/modified"/>
    


    <!-- https://www.gleif.org/ontology/Base/hasAbbreviation -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAbbreviation"/>
    


    <!-- https://www.gleif.org/ontology/Base/hasAbbreviationLocal -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAbbreviationLocal">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAbbreviation"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has abbreviation local</rdfs:label>
        <skos:definition>An abbreviation using a language local to the entity identified; may be English.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAbbreviationTransliterated -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAbbreviationTransliterated">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAbbreviation"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has abbreviation transliterated</rdfs:label>
        <skos:definition>An abbreviation transliterated into a Western alphabet.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAdditionalAddressLine -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAdditionalAddressLine">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has additional address line</rdfs:label>
        <skos:definition>Means to collect the one to three specifically-named optional additional address line elements.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressLine1 -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressLine1">
        <owl:equivalentProperty rdf:resource="https://www.gleif.org/ontology/Base/hasFirstAddressLine"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address line 1</rdfs:label>
        <skos:definition>The mandatory first address line element.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressLine2 -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressLine2">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAdditionalAddressLine"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address line 2</rdfs:label>
        <skos:definition>The second line of the one to three optional additional address line elements.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressLine3 -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressLine3">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAdditionalAddressLine"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address line 3</rdfs:label>
        <skos:definition>The third line of the one to three optional additional address line elements.</skos:definition>
        <skos:note>This element SHALL be omitted if address line 2 is omitted.</skos:note>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressLine4 -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressLine4">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasAdditionalAddressLine"/>
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address line 4</rdfs:label>
        <skos:definition>The fourth line of the one to three optional additional address line elements.</skos:definition>
        <skos:note>This element SHALL be omitted if address line 3 is omitted.</skos:note>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressNumber -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressNumber">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address number</rdfs:label>
        <skos:definition>Optional, additional structured version of an external house number, or range of numbers, contained in one of the address line elements.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasAddressNumberWithinBuilding -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasAddressNumberWithinBuilding">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has address number within building</rdfs:label>
        <skos:definition>Optional, additional structured version of an internal location number, or range of numbers, contained in one of the address line elements.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasCity -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasCity">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/PhysicalAddress"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label>has city</rdfs:label>
        <skos:definition>The mandatory name of the city.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasEnd -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasEnd">
        <rdfs:domain rdf:resource="https://www.gleif.org/ontology/Base/Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
        <rdfs:label xml:lang="en">has end</rdfs:label>
        <skos:definition>The end time of a period.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasEntityExpirationDate -->


