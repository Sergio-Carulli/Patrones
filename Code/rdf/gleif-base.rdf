                    <rdf:Description rdf:about="https://www.gleif.org/ontology/Base/EntityExpirationReasonCorporateAction"/>
                    <rdf:Description rdf:about="https://www.gleif.org/ontology/Base/EntityExpirationReasonDissolved"/>
