        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/base/"/>
        <rdfs:label>has name trading or operating</rdfs:label>
        <skos:altLabel>brand name</skos:altLabel>
        <skos:altLabel>doing business as</skos:altLabel>
        <skos:altLabel>has trading or operating name</skos:altLabel>
        <skos:altLabel>operating under</skos:altLabel>
