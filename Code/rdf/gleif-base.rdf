    </owl:DatatypeProperty>
    


    <!-- https://www.gleif.org/ontology/Base/hasNameLocal -->


    <owl:DatatypeProperty rdf:about="https://www.gleif.org/ontology/Base/hasNameLocal">
        <rdfs:subPropertyOf rdf:resource="https://www.gleif.org/ontology/Base/hasName"/>
        <rdfs:isDefinedBy rdf:resource="https://www.gleif.org/ontology/Base/"/>
