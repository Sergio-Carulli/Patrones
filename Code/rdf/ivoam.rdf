  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger">
    <dc:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0/"/>
    <dc:description>A vocabulary of particles used for observations in astronomy.
This list started its existence as the controlled vocabulary for
VODataService's vs:Waveband type; the machine-readable identifiers are in
upper case for backwards compatibility.</dc:description>
    <ivoasem:vocflavour>RDF Class</ivoasem:vocflavour>
    <dc:created>2020-08-26</dc:created>
    <dc:title xml:lang="en">Messengers</dc:title>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdfs:label xml:lang="en">Messengers</rdfs:label>
    <dc:creator rdf:nodeID="ub12bL14C16"/>
    <dc:creator rdf:nodeID="ub12bL15C5"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#X-ray">
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdfs:comment>           Photon with an energy between 120 eV and 120 keV</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>X-Ray</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub12bL15C5">
    <foaf:name>Plante, R.</foaf:name>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Neutrino">
    <rdfs:label>Neutrino</rdfs:label>
    <rdfs:comment>        This term comprises all generations of neutrinos (electron, µ, τ), and particles as well as antiparticles.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#EUV">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment>      Photon with an energy between 12 eV and 120 eV</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#UV"/>
    <rdfs:label>Extreme UV</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Millimeter">
    <rdfs:comment>      Photon with a wavelength between 0.1 mm and 10 mm (or 30 GHz&lt;=ν&lt;300 GHz)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Millimeter</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Infrared">
    <rdfs:label>Infrared</rdfs:label>
    <rdfs:comment>        Photon with a wavelength between 1 µm and 100 µm</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Optical">
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdfs:comment>         Photon with a wavelength between 300 nm and 1000 nm</rdfs:comment>
    <rdfs:label>Optical</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Photon">
    <rdfs:label>Photon</rdfs:label>
    <rdfs:comment>          Carrier particles of the electromagnetic interaction</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#UV">
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdfs:label>Ultraviolet</rdfs:label>
    <rdfs:comment>     Photon with a wavelength between 100 nm and 300 nm</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Gamma-ray">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdfs:label>Gamma Ray</rdfs:label>
    <rdfs:comment>       Photon with an energy above 120 keV</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ivoa.net/rdf/messenger#Radio">
    <rdfs:comment>           Photon with a wavelength longer than 10 mm (or ν&lt;30 GHz)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ivoa.net/rdf/messenger#Photon"/>
    <rdfs:label>Radio</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/title">
