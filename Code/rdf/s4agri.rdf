    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#sfContains"/>
