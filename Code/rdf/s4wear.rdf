    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Function"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/ElectricalProperty">
    <rdfs:label xml:lang="en">Electrical property</rdfs:label>
    <rdfs:comment xml:lang="en">Class to group those electrical properties related to wearables.</rdfs:comment>
