      <owl:Class rdf:about="https://saref.etsi.org/saref4wear/WearableProperty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/ControllingFunction">
    <rdfs:label xml:lang="en">Controlling function</rdfs:label>
    <rdfs:comment xml:lang="en">A function that allows to control a device from another device.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Function"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
    <rdfs:label xml:lang="en">Person</rdfs:label>
    <rdfs:comment xml:lang="en">A person</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/WearableProperty">
    <rdfs:label xml:lang="en">Wearable property</rdfs:label>
    <rdfs:comment xml:lang="en">Class to group those properties related to wearables.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/Property"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Geometry">
    <rdfs:label xml:lang="en">Geometry</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql#"/>
    <rdfs:comment xml:lang="en">The class represents the top-level geometry type. This class is equivalent to the UML class GM_Object defined in ISO 19107, and it is superclass of all geometry types.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/core/EventFunction">
    <rdfs:label xml:lang="en">Event function</rdfs:label>
    <rdfs:comment xml:lang="en">A function that allows to notify about some relevant activity; e.g., that a certain threshold value has been exceeded or that some object has moved.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Function"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4city/AdministrativeArea">
    <rdfs:label xml:lang="en">Administrative area</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4city/"/>
    <rdfs:comment xml:lang="en">An administrative division, unit, entity, area or region, also referred to as a subnational entity, constituent unit, or country subdivision, is a portion of a country or other region delineated for the purpose of administration. (https://en.wikipedia.org/wiki/Administrative_division).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/BiometricProperty">
    <rdfs:label xml:lang="en">Biometric property</rdfs:label>
    <rdfs:comment xml:lang="en">Class to group those properties related to the biometric characteristics of wearers.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/saref4wear/WearerProperty"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/Wearer">
    <rdfs:label xml:lang="en">Wearer</rdfs:label>
    <rdfs:comment xml:lang="en">A wearer is any living organism that is sensed by a wearable.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://saref.etsi.org/saref4wear/WearerProperty"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/hasProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://saref.etsi.org/saref4wear/LivingOrganism"/>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="https://saref.etsi.org/saref4wear/Wearable"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4wear/featureIsMeasuredByDevice"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/LivingOrganism">
    <rdfs:label xml:lang="en">Living organism</rdfs:label>
    <rdfs:comment xml:lang="en">A living organism is any entity composed of cells.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/FeatureOfInterest"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/Software">
    <rdfs:label xml:lang="en">Software</rdfs:label>
    <rdfs:comment xml:lang="en">A software is a computer program.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/FeatureOfInterest"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/Interface">
    <rdfs:label xml:lang="en">Interface</rdfs:label>
    <rdfs:comment xml:lang="en">An interface is the mechanism with which a device can receive or transmit data to another device.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/core/Measurement">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core/hasValue"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core/hasTimestamp"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/FeatureOfInterest"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasurementOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Represents the measured value made over a property. It is also linked to the unit of measure in which the value is expressed and the timestamp of the measurement.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onClass rdf:resource="https://saref.etsi.org/core/UnitOfMeasure"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://saref.etsi.org/core/UnitOfMeasure"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Measurement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://saref.etsi.org/core/Property"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onClass rdf:resource="https://saref.etsi.org/core/Property"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/EnvironmentalProperty">
