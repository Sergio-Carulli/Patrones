    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4wear/OnBodyWearable"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/core/Property">
    <rdfs:label xml:lang="en">Property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A quality of a feature of interest that can be measured; an aspect of a feature of interest that is intrinsic to and cannot exist without the feature.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Measurement"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToMeasurement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Device"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredByDevice"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Device"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isControlledByDevice"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/core/ActuatingFunction">
    <rdfs:label xml:lang="en">Actuating function</rdfs:label>
    <rdfs:comment xml:lang="en">A function that allows to transmit data to actuators, such as level settings (e.g., temperature) or binary switching (e.g., open/close, on/off).</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Function"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4wear/MemoryStorage">
    <rdfs:label xml:lang="en">Memory storage</rdfs:label>
    <rdfs:comment xml:lang="en">A memory storage is an hardware component intended to store data within the device.</rdfs:comment>
