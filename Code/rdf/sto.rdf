    </owl:NamedIndividual>
    


    <!-- https://w3id.org/i40/sto#RFC_793 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#RFC_793">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <foaf:maker rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <sto:hasClassification rdf:resource="https://w3id.org/i40/sto#IDSRAMSystemLayer"/>
