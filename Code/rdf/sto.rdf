    <!-- https://w3id.org/i40/sto#DataTransferProject -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#DataTransferProject">
        <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
