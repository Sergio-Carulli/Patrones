    <!-- https://w3id.org/i40/sto#RFC_6750 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#RFC_6750">
        <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Publication"/>
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <DUL:associatedWith rdf:resource="https://w3id.org/i40/sto#DtpArchitecture"/>
        <foaf:maker rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <sto:hasClassification rdf:resource="https://w3id.org/i40/sto#IDSRAMSecurity"/>
        <sto:hasPublisher rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <terms:creator rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <terms:description xml:lang="en">This specification describes how to use bearer tokens in HTTP requests to access OAuth 2.0 protected resources.  Any party in possession of a bearer token (a &quot;bearer&quot;) can use it to get access to the associated resources (without demonstrating possession of a cryptographic key).  To prevent misuse, bearer tokens need to be protected from disclosure in storage and in transport.</terms:description>
        <rdfs:comment xml:lang="en">This specification describes how to use bearer tokens in HTTP requests to access OAuth 2.0 protected resources.  Any party in possession of a bearer token (a &quot;bearer&quot;) can use it to get access to the associated resources (without demonstrating possession of a cryptographic key).  To prevent misuse, bearer tokens need to be protected from disclosure in storage and in transport.</rdfs:comment>
        <rdfs:label xml:lang="en">RFC 6750</rdfs:label>
        <skos:altLabel xml:lang="en">OAuth2.0</skos:altLabel>
        <sto:hasDBpediaResource rdf:resource="http://dbpedia.org/resource/OAuth"/>
