    <!-- https://w3id.org/i40/sto#RFC_7644 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#RFC_7644">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Standard"/>
        <foaf:maker rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <sto:hasClassification rdf:resource="https://w3id.org/i40/sto#FiwareSecurity"/>
        <sto:hasDeveloper rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <sto:hasPublisher rdf:resource="https://w3id.org/i40/sto#IETF"/>
        <terms:creator rdf:resource="https://w3id.org/i40/sto#irlan"/>
        <terms:creator rdf:resource="https://w3id.org/i40/sto#sebastian"/>
        <terms:title xml:lang="en">SCIM API</terms:title>
        <rdfs:comment xml:lang="en">The System for Cross-Domain Identity Management (SCIM) specification is an HTTP based protocol that makes managing identities in multi domain scenarios easier to support through a standardized service. Examples include but are not limited to enterprise to cloud service providers, and inter-cloud based scenarios.  The specification suite seeks to build upon experience with existing schemas and deployments, placing specific emphasis on simplicity of development and integration, while applying existing authentication, authorization, and privacy models.  SCIM&apos;s intent is to reduce the cost and complexity of user management operations by providing a common user schema, an extension model, and a service protocol defined by this document.</rdfs:comment>
        <rdfs:label xml:lang="en">RFC 7644</rdfs:label>
        <sto:hasOfficialResource rdf:resource="https://tools.ietf.org/html/draft-ietf-scim-api-19"/>
        <iot:hasTargetConcern rdf:resource="https://w3id.org/i40/sto#IdentityManagement"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/i40/sto#RFC_768 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#RFC_768">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Standard"/>
