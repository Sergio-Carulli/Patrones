    <!-- https://w3id.org/i40/sto#FiwareIotServicesEnablement -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#FiwareIotServicesEnablement">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#FiwareChapter"/>
