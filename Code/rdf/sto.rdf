    <!-- https://w3id.org/i40/sto#SCIM -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#SCIM">
        <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
