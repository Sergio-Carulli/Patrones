        <rdfs:seeAlso>IoT-A Requirement UNI.606</rdfs:seeAlso>
        <rdfs:seeAlso>IoT-A Requirement UNI.623</rdfs:seeAlso>
        <sto:hasDBpediaResource rdf:resource="http://dbpedia.org/resource/Information_privacy"/>
    </owl:NamedIndividual>
    


