    <!-- https://w3id.org/i40/sto#FiwareSecurity -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#FiwareSecurity">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#FiwareChapter"/>
