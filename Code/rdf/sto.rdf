    <!-- https://w3id.org/i40/sto#DataQuality -->


    <owl:NamedIndividual rdf:about="https://w3id.org/i40/sto#DataQuality">
        <rdf:type rdf:resource="https://w3id.org/i40/sto#Concern"/>
